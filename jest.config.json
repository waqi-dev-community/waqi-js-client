{
    "testEnvironment": "node",
    "globals": {
        "__esModule": true
    },
    "transform": {
        "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
        "node_modules/(?!variables/.*)"
    ],
    "moduleFileExtensions": ["js"],
    "testMatch": ["**/__tests__/**/*.js?(x)", "**/?(*.)+(spec|test).js?(x)"]
}
